<IMFRoot>
	<SysInfo>
		<MessageSequenceID>@MessageSequenceID@</MessageSequenceID>
		<MessageType>FlightData</MessageType>
		<ServiceType>FUS</ServiceType>
		<OperationMode>@OperationMode@</OperationMode>
		<SendDateTime>@SendDateTime@</SendDateTime>
		<CreateDateTime>@CreateDateTime@</CreateDateTime>
		<OriginalDateTime>@OriginalDateTime@</OriginalDateTime>
		<Receiver>IMF</Receiver>
		<Sender>VZ</Sender>
		<Owner>VZ</Owner>
	</SysInfo>
	<Data>
		<PrimaryKey>
			<FlightKey>
				<FlightScheduledDate>@FlightScheduledDate@</FlightScheduledDate>
				<FlightIdentity>@FlightIdentity@</FlightIdentity>
				<FlightDirection>@FlightDirection@</FlightDirection>
			</FlightKey>
		</PrimaryKey>
		<FlightData>
			<General>
				<FlightScheduledDateTime>@FlightScheduledDateTime@</FlightScheduledDateTime>
				<Registration>@Registration@</Registration>
				<CallSign>@CallSign@</CallSign>
				<AircraftType>
					<AircraftICAOCode>@AircraftICAOCode@</AircraftICAOCode>
				</AircraftType>
				<FlightServiceType>
					<FlightCAACServiceType>@FlightServiceType@</FlightCAACServiceType>
				</FlightServiceType>
				<FlightRoute>
					<IATARoute>
						<IATAOriginAirport>@IATAOriginAirport@</IATAOriginAirport>
						<IATAPreviousAirport>@IATAPreviousAirport@</IATAPreviousAirport>
						<IATAFullRoute>
							<AirportIATACode LegNo="1">@AirportIATACodeLeg1@</AirportIATACode>
							<AirportIATACode LegNo="2">@AirportIATACodeLeg2@</AirportIATACode>
							<AirportIATACode LegNo="3">@AirportIATACodeLeg3@</AirportIATACode>
							<AirportIATACode LegNo="99">@AirportIATACodeLeg4@</AirportIATACode>
						</IATAFullRoute>
						<IATANextAirport>@IATANextAirport@</IATANextAirport>
						<IATADestinationAirport>@IATADestinationAirport@</IATADestinationAirport>
					</IATARoute>
				</FlightRoute>
				<FreeTextComment>
                            <PublicTextComment>@PublicTextComment@</PublicTextComment>
                            <CreateReason>@CreateReason@</CreateReason>
                  </FreeTextComment>
			</General>
			<OperationalDateTime>
				<PreviousAirportDepartureDateTime>
					<ScheduledPreviousAirportDepartureDateTime>@ScheduledPreviousAirportDepartureDateTime@</ScheduledPreviousAirportDepartureDateTime>
					<EstimatedPreviousAirportDepartureDateTime>@EstimatedPreviousAirportDepartureDateTime@</EstimatedPreviousAirportDepartureDateTime>
					<ActualPreviousAirportDepartureDateTime>@ActualPreviousAirportDepartureDateTime@</ActualPreviousAirportDepartureDateTime>
				</PreviousAirportDepartureDateTime>
				<LandingDateTime>
					<ScheduledLandingDateTime>@ScheduledLandingDateTime@</ScheduledLandingDateTime>
					<EstimatedLandingDateTime>@EstimatedLandingDateTime@</EstimatedLandingDateTime>
					<ActualLandingDateTime>@ActualLandingDateTime@</ActualLandingDateTime>
				</LandingDateTime>
				<OnBlockDateTime>
					<ScheduledOnBlockDateTime>@ScheduledOnBlockDateTime@</ScheduledOnBlockDateTime>
					<EstimatedOnBlockDateTime>@EstimatedOnBlockDateTime@</EstimatedOnBlockDateTime>
					<ActualOnBlockDateTime>@ActualOnBlockDateTime@</ActualOnBlockDateTime>
				</OnBlockDateTime>
				<OffBlockDateTime>
					<ScheduledOffBlockDateTime>@ScheduledOffBlockDateTime@</ScheduledOffBlockDateTime>
					<EstimatedOffBlockDateTime>@EstimatedOffBlockDateTime@</EstimatedOffBlockDateTime>
					<ActualOffBlockDateTime>@ActualOffBlockDateTime@</ActualOffBlockDateTime>
				</OffBlockDateTime>
				<TakeOffDateTime>
					<ScheduledTakeOffDateTime>@ScheduledTakeOffDateTime@</ScheduledTakeOffDateTime>
					<ActualTakeOffDateTime>@ActualTakeOffDateTime@</ActualTakeOffDateTime>
				</TakeOffDateTime>
				<FlyTime>
					<ScheduledFlyTime>@ScheduledFlyTime@</ScheduledFlyTime>
					<EstimatedFlyTime>@EstimatedFlyTime@</EstimatedFlyTime>
				</FlyTime>
				<DoorOpenDateTime>
					<ActualDoorOpenDateTime>@ActualDoorOpenDateTime@</ActualDoorOpenDateTime>
				</DoorOpenDateTime>
				<DoorCloseDateTime>
					<ActualCloseDateTime>@ActualCloseDateTime@</ActualCloseDateTime>
				</DoorCloseDateTime>
				<OnBridgeDateTime>
					<ActualOnBridgeDateTime>@ActualOnBridgeDateTime@</ActualOnBridgeDateTime>
				</OnBridgeDateTime>
				<OffBridgeDateTime>
					<ActualOffBridgeDateTime>@ActualOffBridgeDateTime@</ActualOffBridgeDateTime>
				</OffBridgeDateTime>
				<NextAirportArrivalDateTime>
					<ScheduledNextAirportArrivalDateTime>@ScheduledNextAirportArrivalDateTime@</ScheduledNextAirportArrivalDateTime>
					<ActualNextAirportArrivalDateTime>@ActualNextAirportArrivalDateTime@</ActualNextAirportArrivalDateTime>
				</NextAirportArrivalDateTime>
			</OperationalDateTime>
			<Status>
				<OperationStatus>@OperationStatus@</OperationStatus>
			</Status>
		</FlightData>
	</Data>
</IMFRoot>
